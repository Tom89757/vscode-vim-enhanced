{
  "name": "vscode-vim-enhanced",
  "displayName": "vscode-vim-enhanced",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "vscodeVimEnhanced",
      "properties": {
        "vscodeVimEnhanced.charPrimaryColor": {
          "type": "string",
          "default": "red",
          "description": "The color of the char that can be reached with the f motion within one jump"
        },
        "vscodeVimEnhanced.charSecondaryColor": {
          "type": "string",
          "default": "green",
          "description": "The color of the char that can be reached with the f motion with more than one jump"
        },
        "vscodeVimEnhanced.charFontWeight": {
          "type": "string",
          "default": "400",
          "description": "The font weight of the char that can be reached with the f motion",
          "enum": [
            "100",
            "200",
            "300",
            "400",
            "500",
            "600",
            "700",
            "800",
            "900"
          ]
        },
        "vscodeVimEnhanced.enableUnderline": {
          "type": "boolean",
          "default": true,
          "description": "If true, a line will be drawn under the highlighted characters"
        }
      }
    },
    "commands": [
      {
        "command": "vscodeVimEnhanced.enhanceSKey",
        "title": "Enhance Vim S Key"
      },
      {
        "command": "vscodeVimEnhanced.enhanceBackSKey",
        "title": "Enhance Vim Back S Key"
      },
      {
        "command": "vscodeVimEnhanced.enhanceFKey",
        "title": "Enhance Vim F Key"
      },
      {
        "command": "vscodeVimEnhanced.enhanceBackFKey",
        "title": "Enhance Vim Back F Key"
      },
      {
        "command": "vscodeVimEnhanced.removeEnhanceSKeyHighlight",
        "title": "Remove Enhance S Key Highlight"
      },
      {
        "command": "vscodeVimEnhanced.removeEnhanceBackSKeyHighlight",
        "title": "Remove Enhance Back S Key Highlight"
      },
      {
        "command": "vscodeVimEnhanced.removeEnhanceFKeyHighlight",
        "title": "Remove Enhance F Key Highlight"
      },
      {
        "command": "vscodeVimEnhanced.removeEnhanceBackFKeyHighlight",
        "title": "Remove Enhance Back F Key Highlight"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "extensionDependencies": [
    "vscodevim.vim"
  ]
}
